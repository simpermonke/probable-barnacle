from tkinter import *
import tkinter as tk
class HomePage(Frame):
    def __init__(self, master):
        Frame.__init__(self, master)
        #make the buttons
        #do the same as with the last three GUI's
        self.button1 = Button(master, bg= "green", relief = "solid", width = 18, height = 10, text = "Guitar", font = "Arial")
        self.button1.pack()
        self.button1.place(x=-0, y=0)
        self.button2 = Button(master, bg= "red", relief = "solid", width = 18, height = 10, text = "Piano", font = "Arial")
        self.button2.pack()
        self.button2.place(x=200, y=0)
        self.button3 = Button(master, bg= "blue", relief = "solid", width = 18, height = 10, text = "Drum", font = "Arial")
        self.button3.pack()
        self.button3.place(x=400, y=0)
    #connect the previous GUI's to be linked in this one
    def handleButtonPress(self):
        pass
window = Tk()
#make window size
window.geometry("600x300")
window.title = ('Homepage')
h = HomePage(window)
window.mainloop()
